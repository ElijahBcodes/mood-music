  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@rails/activestorage": "^7.0.4",
    "@rails/ujs": "^7.0.4",
    "@rails/webpacker": "5.4.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.2.1",
    "animate.css": "^4.1.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "bootstrap": "^5.2.2",
    "global-jsdom": "^8.5.0",
    "list": "^2.0.19",
    "node": "^19.3.0",
    "nvm": "^0.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-image-button": "^1.0.6",
    "react-media-player": "^0.7.9",
    "react-particles": "^2.3.3",
    "react-player": "^2.11.0",
    "react-router-dom": "^6.4.1",
    "react-tsparticles": "^2.3.3",
    "react_ujs": "^2.6.2",
    "reactstrap": "^9.1.4",
    "tsparticles": "^2.3.4",
    "tsparticles-engine": "^2.3.3",
    "tsparticles-path-curves": "^2.3.3",
    "tsparticles-preset-big-circles": "^2.3.4",
    "tsparticles-preset-sea-anemone": "^2.3.4",
    "tsparticles-preset-triangles": "^2.3.3",
    "version": "^0.1.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "webpack-dev-server": "^3"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "webpack"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "app/javascript/components"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/javascript/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/javascript/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
